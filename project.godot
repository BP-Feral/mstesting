"res://_icons/": "gray",
"res://_singletons/": "gray",
"res://_splash_screen/": "gray",
"res://assets/": "yellow",
"res://scenes/": "pink",
"res://scripts/": "blue",
"res://ui/": "green"
}

[rendering]

textures/canvas_textures/default_texture_filter=0
environment/defaults/default_clear_color=Color(0, 0, 0, 1)
